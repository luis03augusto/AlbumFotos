<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AlbumFotos</title>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/vendor/materialize-css/css/materialize.min.css" rel="stylesheet" />
    <link href="~/vendor/font-awesome-5-css/css/fontawesome.min.css" rel="stylesheet" />
    <environment include="Development">
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body>

    <div class="">
        <nav class="blue darken-3">
            <div class="nav-wrapper">
                <a  class="brand-logo center" asp-controller="Album" asp-action="Index">Album de fotos</a>

            </div>
        </nav>
    </div>


    <div class="">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - AlbumFotos</p>
        </footer>
    </div>

    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/materialize-css/js/materialize.min.js"></script>
    <script src="~/js/site.min.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
